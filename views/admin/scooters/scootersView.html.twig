{% set navActive = "scooters" %}

{% extends "templates/admin.html.twig" %}

{% block content %}
    <section id="container-admin">

       {{ include('includes/boxMessages.html.twig') }}

        <header id="admin-header">
            <h1>Voir les informations d'un abonnement</h1>
        </header>

        {% include "admin/scooters/navAdminScooters.html.twig" with {active : "view"} %}

        <section id="admin-content">
                <div class="box-input-txt">
                    <label>
                        <div>Id</div>
                        <input value="{{ scooter.id ?? '' }}" readonly>
                    </label>
                </div>

                <div class="box-input-txt">
                    <label>
                        <div>Batterie</div>
                        <input type="text" name="battery" value="{{ scooter.battery ?? '' }}" readonly>
                    </label>
                </div>

                <div class="box-input-txt">
                    <label>
                        <div>Status</div>
                        <input type="text" value="{{ scooter.status ?? '' }}" readonly>
                    </label>
                </div>

        </section>
    </section>
{% endblock content %}